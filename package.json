{
  "name": "community",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --respawn --transpileOnly -- app.ts",
    "build:server": "tsc",
    "build:client": "cd client && yarn && yarn run build",
    "dev": "concurrently \"yarn run dev:server\" \"cd client && yarn start\"",
    "build": "yarn run build:server && yarn run build:client",
    "start": "node build/app.js",
    "eslint": "eslint --fix '**/*.ts?(x)'"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/image-size": "^0.8.0",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.12",
    "@types/multer": "^1.4.3",
    "@types/node": "^13.13.5",
    "azure-storage": "^2.10.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "image-size": "^0.8.3",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.6",
    "mongoose": "^5.9.9",
    "multer": "^1.4.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
